{
  "name": "monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "check": "biome check --write .",
    "dev": "nx run-many -t dev",
    "build": "nx run-many -t build",
    "check-types": "nx run-many -t check-types",
    "dev:native": "nx dev native",
    "dev:web": "nx dev web",
    "dev:server": "nx dev @monorepo/backend",
    "dev:setup": "nx dev:setup @monorepo/backend",
    "clean": "rm -rf node_modules dist pnpm-lock.yaml && pnpm store prune & find . -type d \\( -name 'node_modules' -o -name 'dist' -o -name '.output' -o -name '.vinxi' -o -name '.cache' -o -name '.expo' -o -name '.nx' \\) -prune -exec rm -rf {} + & wait"
  },
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "ultracite": "5.1.1",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "nx": "21.3.11"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "pnpm dlx ultracite format"
    ]
  },
  "packageManager": "pnpm@10.14.0"
}
