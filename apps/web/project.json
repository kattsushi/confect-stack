{
  "$schema": "./../../node_modules/nx/schemas/project-schema.json",
  "name": "@monorepo/web",
  "projectType": "application",
  "sourceRoot": "apps/web/src",
  "targets": {
    "dev:watch": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["pnpm nx dev web", "nx run @monorepo/web:watch-deps "],
        "cwd": "apps/web",
        "parallel": true
      }
    },
    "dev:server:watch": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "pnpm nx serve web",
          "pnpm vite",
          "pnpm nx run @monorepo/web:watch-deps "
        ],
        "cwd": "apps/web",
        "parallel": true
      }
    }
  },
  "tags": ["react-start"]
}
