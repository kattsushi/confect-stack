{
  "name": "@monorepo/native",
  "sourceRoot": "apps/native",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo start --clear",
        "cwd": "apps/native"
      },
      "dependsOn": ["^build"]
    },
    "android": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo run:android",
        "cwd": "apps/native"
      }
    },
    "ios": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo run:ios",
        "cwd": "apps/native"
      }
    },
    "prebuild": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo prebuild",
        "cwd": "apps/native"
      }
    },
    "web": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo start --web",
        "cwd": "apps/native"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo export",
        "cwd": "apps/native"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "check-types": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "apps/native"
      }
    }
  },
  "tags": ["type:app", "platform:mobile"]
}
